<h1>Login</h1>
<%= form_with url: session_path, method: :post do |f| %>
  <p>
    <%= f.label :email %><br>
    <%= f.email_field :email %>
  </p>
  <p>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </p>
  <%= f.submit 'Log in' %>
<% end %>
<p>New user? <%= link_to 'Sign up', new_user_path %></p>
