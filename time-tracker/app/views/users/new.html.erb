<h1>Sign Up</h1>
<%= form_with model: @user do |f| %>
  <p>
    <%= f.label :email %><br>
    <%= f.email_field :email %>
  </p>
  <p>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.label :password_confirmation %><br>
    <%= f.password_field :password_confirmation %>
  </p>
  <%= f.submit 'Sign Up' %>
<% end %>
<p><%= link_to 'Login', new_session_path %></p>
